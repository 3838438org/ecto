set(ECTO_VERSION @ECTO_VERSION@)
set(PACKAGE_VERSION_EXACT False)
set(PACKAGE_VERSION_COMPATIBLE False)
message(STATUS "This is ecto version: ${ECTO_VERSION}")
if("${PACKAGE_FIND_VERSION}" STREQUAL "")
set(PACKAGE_VERSION_COMPATIBLE TRUE)
message(STATUS "No version requested, compatible")
return()
endif()
message(STATUS "Trying to find ecto version: ${PACKAGE_FIND_VERSION}")
if(${PACKAGE_FIND_VERSION} VERSION_EQUAL ${ECTO_VERSION})
  set(PACKAGE_VERSION_EXACT True)
  set(PACKAGE_VERSION_COMPATIBLE True)
endif()

if(${PACKAGE_FIND_VERSION} VERSION_LESS ${ECTO_VERSION})
  set(PACKAGE_VERSION_COMPATIBLE True)
endif()
