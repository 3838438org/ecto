SET(ECTO_VERSION @ECTO_VERSION@)
set(ECTO_MAJOR_VERSION @ECTO_MAJOR_VERSION@)
set(ECTO_MINOR_VERSION @ECTO_MINOR_VERSION@)
set(ECTO_PATCH_VERSION @ECTO_PATCH_VERSION@)

set(PACKAGE_VERSION ${ECTO_VERSION})
if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL ${ECTO_MAJOR_VERSION})
  set(PACKAGE_VERSION_COMPATIBLE True) # compatible with any version @OPENCV_VERSION_MAJOR@.x.x
  if("${PACKAGE_FIND_VERSION_MINOR}" EQUAL ${ECTO_MINOR_VERSION})
    set(PACKAGE_VERSION_COMPATIBLE True)    # compatible with any version @OPENCV_VERSION_MAJOR@.@OPENCV_VERSION_MINOR@.x
    if("${PACKAGE_FIND_VERSION_PATCH}" EQUAL ${ECTO_PATCH_VERSION})
      set(PACKAGE_VERSION_EXACT True)    # exact match for patch @OPENCV_VERSION_MAJOR@.@OPENCV_VERSION_MINOR@.@OPENCV_VERSION_PATCH@
    endif()
 endif()
endif()
